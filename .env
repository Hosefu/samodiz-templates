# .env
# Общие настройки
DJANGO_SECRET_KEY=your-secret-key-here
DEBUG=1

# База данных
DB_NAME=samodesdb
DB_USER=postgres
DB_PASSWORD=postgres
DB_HOST=db
DB_PORT=5432

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# JWT
JWT_SECRET=your-jwt-secret-key

# API Keys
API_KEY=development-api-key

# Порты
FRONTEND_PORT=5173
BACKEND_PORT=8000
PDF_PORT=8081
PNG_PORT=8082

# PNG Renderer дополнительные настройки
PNG_RENDERER_BROWSER_HEADLESS=True
PNG_RENDERER_MAX_CONCURRENT_BROWSERS=5

# MinIO Configuration
MINIO_ENDPOINT=minio:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin123
MINIO_SECURE=False
MINIO_REGION=eu-west-1
MINIO_BUCKET_TEMPLATES=templates-assets
MINIO_BUCKET_DOCUMENTS=generated-documents